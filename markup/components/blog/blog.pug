mixin blog(data)
    section#blogs.blogs.animate__animated
        .blogs__container.container

            .blogs__circle.circle

            != pugHelpers.Icon.call(locals, {iconName: 'reviews-path', className: 'reviews__path'})


            h2.blogs__title.title.title_secondary  <span>06</span> Блог.

            .blogs__slider
                .swiper
                    .swiper-wrapper
                        .blogs__review.swiper-slide
